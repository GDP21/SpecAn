METAC 2 1

/* Create TBI data structure (used by MeOS) */
TBI INT  THREAD 0 CCB 32
TBI BGND THREAD 0 CCB 32

/**************************************************************/

EXEC release/thread0.elf	THREAD 0
STACK 0x1000				THREAD 0
HEAP  0x1000				THREAD 0

GBLHEAP 0x4000

LAYOUT
{
	0xB7000400	GBLHEAP
				THREAD 0 STACK
				THREAD 0 HEAP
				THREAD 0 EXEC CODE
				THREAD 0 EXEC DATA
}
