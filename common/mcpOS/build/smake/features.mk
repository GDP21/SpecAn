ifneq ($(filter $(FEATURES), FAST_INTS),)
MCPOS_ISUFFIX=M
else
MCPOS_ISUFFIX=A
endif
ifeq ($(OPERATING_SYSTEM),MEOS)
MCPOS_OSSUFFIX=M
else
MCPOS_OSSUFFIX=N
endif
ifneq (,$(findstring MCPOS_DCP_LOGGING,$(DEFINES)))
MCPOS_UNIT_NAME_SUFFIX=_dl
endif

UNIT_NAME_SUFFIX=_$(UCCP_NAME)$(MCPOS_UNIT_NAME_SUFFIX)_$(MCPOS_OSSUFFIX)$(MCPOS_ISUFFIX)
